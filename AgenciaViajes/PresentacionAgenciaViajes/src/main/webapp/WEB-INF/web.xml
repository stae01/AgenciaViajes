<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
    <servlet>
        <servlet-name>AgregarClienteServlet</servlet-name>
        <servlet-class>servletsClientes.AgregarClienteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarAvionesServlet</servlet-name>
        <servlet-class>servletsAviones.AgregarAvionesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuscarAvionesServlet</servlet-name>
        <servlet-class>servletsAviones.BuscarAvionesServlet</servlet-class>
    </servlet>
    
    
    
     <servlet-mapping>
        <servlet-name>AgregarClienteServlet</servlet-name>
        <url-pattern>/agregarClienteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarAvionesServlet</servlet-name>
        <url-pattern>/agregarAvionesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscarAvionesServlet</servlet-name>
        <url-pattern>/buscarAvionesServlet</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
